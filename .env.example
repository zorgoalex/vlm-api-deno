# ZAI API (по умолчанию)
ZAI_API_KEY=sk-your-zai-key-here
DEFAULT_MODEL=glm-4.6v-flash

# BigModel API (опционально)
# BIGMODEL_API_KEY=sk-your-bigmodel-key-here

# OpenRouter API (опционально)
# OPENROUTER_API_KEY=sk-or-your-openrouter-key-here

# Admin
ADMIN_TOKEN=your-secure-random-token-here

# App Info
APP_URL=http://localhost:8000
APP_TITLE=VLM Proxy Deno (Dev)

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Security (опционально)
# WRITE_RL_LIMIT=20
# WRITE_RL_WINDOW_SEC=60
# ENABLE_NONCE=1
# NONCE_TTL_SEC=300

# Supabase (опционально, для бэкапов)
# SUPABASE_URL=https://xxx.supabase.co
# SUPABASE_SERVICE_KEY=eyJ...

# Cloudflare R2 (optional, image upload as URL)
# R2_ACCOUNT_ID=your-cloudflare-account-id
# R2_ACCESS_KEY_ID=your-r2-access-key-id
# R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
# R2_BUCKET=tmp-images
# Optional: if bucket/domain is public, return direct URL instead of presigned URL
# R2_PUBLIC_BASE_URL=https://pub-xxxx.r2.dev
